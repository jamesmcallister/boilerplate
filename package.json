{
  "name": "boilerplate",
  "version": "0.0.1",
  "description": "boilerplate",
  "scripts": {
    "lint": "eslint --fix src/**",
    "kill": "netstat -plten |grep node",
    "purge": "rm -rf node_modules dist",
    "clean": "npm run purge && npm install",
    "build": "rm -rf dist && webpack --config bin/webpack.config.js --progress -p",
    "build:start": "npm run build && node bin/serve-dist-folder.js",
    "start": "webpack-dev-server --hot --config bin/webpack.dev.config.js",
    "rollup": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamesmcallister/boilerplate.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "James McAllister",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/jamesmcallister/issues"
  },
  "homepage": "https://github.com/jamesmcallister",
  "devDependencies": {
    "autoprefixer": "6.3.7",
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.23.1",
    "eslint": "3.0.1",
    "eslint-config-standard": "5.3.1",
    "eslint-config-standard-react": "2.5.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-promise": "1.3.2",
    "eslint-plugin-react": "5.2.2",
    "eslint-plugin-standard": "1.3.2",
    "express": "4.14.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "npm-install-webpack-plugin": "4.0.4",
    "postcss-loader": "0.9.1",
    "precss": "1.4.0",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "react-router": "2.5.2",
    "style-loader": "0.13.1",
    "tachyons": "4.0.1",
    "webpack": "2.1.0-beta.15",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "2.1.0-beta.0",
    "webpack-hot-middleware": "2.12.1"
  }
}
