{
  "name": "boilerplate",
  "version": "0.0.1",
  "description": "boilerplate",
  "scripts": {
    "purge": "rm -rf node_modules dist",
    "clean": "npm run purge && npm install",
    "build": "rm -rf dist && webpack --config webpack.dev.config.js -p",
    "start": "webpack-dev-server --config webpack.dev.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamesmcallister/boilerplate.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "James McAllister",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/jamesmcallister/issues"
  },
  "homepage": "https://github.com/jamesmcallister",
  "devDependencies": {
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015-native-modules": "6.6.0",
    "babel-preset-es2015-rollup": "1.1.1",
    "babel-preset-react": "6.11.1",
    "eslint": "3.0.1",
    "eslint-plugin-react": "5.2.2",
    "html-webpack-plugin": "2.22.0",
    "react": "15.2.0",
    "react-dom": "15.2.0",
    "react-router": "2.5.2",
    "webpack": "2.1.0-beta.15",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "2.1.0-beta.0",
    "webpack-hot-middleware": "2.12.1"
  }
